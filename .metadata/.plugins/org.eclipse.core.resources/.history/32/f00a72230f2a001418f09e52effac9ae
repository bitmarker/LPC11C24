#if defined (__USE_LPCOPEN)
#include "chip.h"
#endif

#include <cr_section_macros.h>


int main(void) {



    // Force the counter to be placed into memory
    volatile static int i = 0 ;

    // Enter an infinite loop, just incrementing a counter
    while(1) {
        i++ ;
    }

    return 0 ;
}
